{
  "name": "flapi-cms-designsystem-testapp",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "web:run:dev:local": "nuxi dev --dotenv .env.development --open --no-clear --clipboard --port=1470",
    "web:run:dev:remote": "nuxi dev --dotenv .env.development-remote --open --no-clear --clipboard --port=1470",
    "web:run:dev:docker:local": "cross-env ENV_MODE=local docker-compose up",
    "web:run:dev:docker:remote": "cross-env ENV_MODE=remote docker-compose up",
    "web:build:static:develop": "nuxi generate --dotenv .env.development",
    "web:build:static:staging": "nuxi generate --dotenv .env.staging",
    "web:build:static:prod": "nuxi generate --dotenv .env.production",
    "web:build:server:develop": "nuxi build --dotenv .env.development",
    "web:build:server:staging": "nuxi build --dotenv .env.staging",
    "web:build:server:prod": "nuxi build --dotenv .env.production",
    "web:preview": "cross-env PORT=1462 nuxt preview --dotenv .env.development",
    "web:analyze": "nuxi analyze --loglevel verbose",
    "web:cleanup": "nuxi cleanup",
    "web:prepare": "nuxi prepare",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:ts",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "lint:ts": "vue-tsc --noEmit",
    "lint:fix": "prettier --write --list-different . && npm run lint:prettier --fix && npm run lint:eslint --fix",
    "prepare": "nuxi prepare"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.7.0",
    "@vee-validate/i18n": "^4.14.7",
    "@vee-validate/rules": "^4.14.7",
    "axios": "^1.7.7",
    "luxon": "^3.5.0",
    "pinia": "^2.2.6",
    "vee-validate": "^4.14.7",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@nuxt/devtools": "^1.6.0",
    "@nuxt/test-utils": "^3.15.1",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@stylistic/eslint-plugin": "^2.10.1",
    "@types/jsdom": "^21.1.7",
    "@types/luxon": "^3.4.2",
    "@types/mochawesome": "^6.2.4",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-vue": "^9.30.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "mochawesome": "^7.1.3",
    "nitropack": "^2.10.3",
    "npm-check-updates": "^17.1.1O",
    "nuxt": "^3.14.159",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "serve": "^14.2.4",
    "typescript": "~5.5.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.10"
  },
  "engines": {
    "node": ">=22.11.0"
  }
}
